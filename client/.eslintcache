>\r\n  );\r\n}\r\n\r\nNavbar.propTypes = {\r\n  logout: PropTypes.func.isRequired,\r\n  auth: PropTypes.object.isRequired,\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n   auth: state.auth\r\n})\r\n\r\nexport default connect(mapStateToProps, {logout})(Navbar);\r\n","C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\components\\layout\\Landing.js",["140","141"],"import React, { Component } from 'react';\r\nimport {Link, Redirect} from \"react-router-dom\";\r\nimport {connect} from \"react-redux\";\r\nimport Login from \"../auth/Login\";\r\nimport PropTypes from 'prop-types'\r\n\r\n\r\n\r\nconst Landing = ({isAuthenticated}) => {\r\n  if(isAuthenticated) {\r\n    <Redirect to={'/dashboard'}/>\r\n  }\r\n\r\n    return (\r\n      <section className=\"landing\">\r\n        <div className=\"dark-overlay landing-inner text-light\">\r\n          <div className=\"container\">\r\n            <div className=\"row\">\r\n              <div className=\"col-md-12 text-center\">\r\n                <h1 className=\"display-3 mb-4\">Developer Connector</h1>\r\n                <p className=\"lead\">\r\n                  Create a developer profile/portfolio, share posts and get help\r\n                  from other developers\r\n                </p>\r\n                <Link to=\"/register\" className=\"btn btn-lg btn-info mr-2\">\r\n                  Sign Up\r\n                </Link>\r\n                <Link to=\"/login\" className=\"btn btn-lg btn-light\">\r\n                  Login\r\n                </Link>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    );\r\n}\r\n\r\nLanding.propTypes = {\r\n  isAuthenticated: PropTypes.bool\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n  isAuthenticated:  state.auth.isAuthenticated\r\n})\r\n\r\nexport default connect(mapStateToProps)(Landing);\r\n","C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\components\\auth\\Register.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\components\\auth\\Login.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\store.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\reducers\\index.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\reducers\\alert.js",["142"],"import { SET_ALERT, REMOVE_ALERT } from '../actions/types'\r\nconst initialState = []\r\n\r\nexport default function (state = initialState, action) {\r\n  const {type, payload} = action\r\n  switch(type) {\r\n    case SET_ALERT:\r\n      return [...state, payload]\r\n    case REMOVE_ALERT:\r\n      return state.filter(alert => alert.id !== payload)\r\n    default:\r\n      return state;\r\n  }\r\n}\r\n","C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\actions\\types.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\actions\\alert.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\components\\layout\\Alert.js",["143"],"import React, {PureComponent} from 'react';\r\nimport PropTypes from 'prop-types';\r\nimport {connect} from \"react-redux\";\r\n\r\nconst Alert = ({alerts}) =>\r\n  alerts !== null &&\r\n  alerts.length > 0 &&\r\n  alerts.map(alert => (\r\n  <div key={alert.id} className={`alert alert-${alert.alertType}`}>\r\n    {alert.msg}\r\n  </div>\r\n))\r\n\r\nAlert.propTypes = {\r\n  alerts: PropTypes.array.isRequired\r\n};\r\n\r\nconst mapStateToProps = state => ({\r\n  alerts: state.alert\r\n})\r\n\r\nexport default connect(mapStateToProps)(Alert);\r\n","C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\actions\\auth.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\reducers\\auth.js",["144"],"import {\r\n  REGISTER_FAIL,\r\n  REGISTER_SUCCESS,\r\n  USER_LOADED,\r\n  AUTH_ERROR,\r\n  LOGIN_SUCCESS,\r\n  LOGIN_FAIL,\r\n  LOGOUT,\r\n  DELETE_ACC\r\n} from \"../actions/types\";\r\n\r\nconst initialState = {\r\n  token: localStorage.getItem('token'),\r\n  isAuthenticated: null,\r\n  loading: true,\r\n  user: null\r\n}\r\n\r\nexport default function (state = initialState, action) {\r\n  const {type, payload} = action\r\n\r\n  switch (type) {\r\n    case USER_LOADED:\r\n      return {\r\n        ...state,\r\n        isAuthenticated: true,\r\n        loading: false,\r\n        user: payload\r\n      }\r\n    case REGISTER_SUCCESS:\r\n    case LOGIN_SUCCESS:\r\n      localStorage.setItem('token', payload.token)\r\n      return {\r\n        ...state,\r\n        ...payload,\r\n        isAuthenticated: true,\r\n        loading: false\r\n      }\r\n    case REGISTER_FAIL:\r\n    case AUTH_ERROR:\r\n    case LOGIN_FAIL:\r\n    case LOGOUT:\r\n    case DELETE_ACC:\r\n      localStorage.removeItem('token')\r\n      return {\r\n        ...state,\r\n        token:null,\r\n        isAuthenticated: false,\r\n        loading: false\r\n      }\r\n    default:\r\n      return state\r\n  }\r\n}\r\n\r\n","C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\utils\\setAuthToken.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\components\\dashboard\\Dashboard.js",[],"C:\\Users\\reeke\\WebstormProjects\\brad.travesty-MERN\\devconnector_S2\\client\\src\\components\\routing\\PrivateRoute.js",["145"],"import React, {Component} from 'react';\r\nimport PropTypes from 'prop-types';\r\nimport {connect} from \"react-redux\";\r\nimport {Route, Redirect} from 'react-router-dom'\r\n\r\nconst PrivateRoute = ({\r\n                        component: Component,\r\n                        auth: {isAuthenticated, loading},\r\n                        ...rest\r\n}) => (\r\n  <Route\r\n    {...rest}\r\n    render={props =>\r\n